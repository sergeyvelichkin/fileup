{
  "name": "react-node-aws-s3-uploader",
  "version": "1.0.0",
  "description": "Uploader to S3",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client:build": "cd client && npm run build"
  },
  "engines": {
    "node": "10.10.0"
  },
  "author": "Sergei Velichkin",
  "dependencies": {
    "aws-sdk": "^2.329.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "mongoose": "^5.7.5",
    "multer": "^1.4.0",
    "multer-s3": "^2.7.0",
    "nodemon": "^2.0.4",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "url": "^0.11.0",
    "validator": "^10.8.0"
  }
}
